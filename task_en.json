{
  "atomCode": "uploadReport",
  "execution": {
    "packagePath": "uploadReport-jar-with-dependencies.jar",
    "language": "java",
    "minimumVersion": "1.8",
    "demands": [],
    "target": "$bk_java_path -jar uploadReport-jar-with-dependencies.jar -Dfile.encoding=utf8"
  },
  "input": {
    "fileDir": {
      "label": "Output report path to be displayed (all sub-files under this path will be archived)",
      "default": "",
      "placeholder": "./report/",
      "type": "vuex-input",
      "desc": " Support automatic rendering of html files ",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "indexFile": {
      "label": "entry file",
      "default": "",
      "placeholder": "index.html",
      "type": "vuex-input",
      "desc": " Support automatic rendering of html files  ",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "indexFileCharset": {
      "label": "Entry file encoding",
      "default": "UTF-8",
      "placeholder": "UTF-8",
      "type": "selector",
      "desc": "Entry file encoding",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "options": [
        {
          "id": "UTF-8",
          "name": "UTF-8"
        },
        {
          "id": "Big5",
          "name": "Big5"
        },
        {
          "id": "GB2312",
          "name": "GB2312"
        },
        {
          "id": "GBK",
          "name": "GBK"
        },
        {
          "id": "UTF-16",
          "name": "UTF-16"
        },
        {
          "id": "UTF-16BE",
          "name": "UTF-16BE"
        },
        {
          "id": "UTF-16LE",
          "name": "UTF-16LE"
        },
        {
          "id": "ISO-8859-1",
          "name": "ISO-8859-1"
        },
        {
          "id": "US-ASCII",
          "name": "US-ASCII"
        },
        {
          "id": "default",
          "name": "System default encoding"
        }
      ]
    },
    "reportName": {
      "label": "label alias",
      "default": "",
      "placeholder": "Please enter a custom report name",
      "type": "vuex-input",
      "desc": " Support automatic rendering of html files  ",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "isParallel": {
      "text": "Enable parallel upload",
      "default": false,
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false
    },
    "isSendEmail": {
      "text": "Enable email notification",
      "default": true,
      "type": "atom-checkbox",
      "required": false,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "desc": "The length of the mail content is limited to 10MB"
    },
    "receivers": {
      "label": "Receivers, support the email group",
      "default": "",
      "type": "user-input",
      "inputType": "all",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "isSendEmail",
            "value": true
          }
        ]
      }
    },
    "body": {
      "label": "Email Subject",
      "default": "【${pipeline.name}】  #${pipeline.build.num} The custom report has been archived",
      "type": "vuex-input",
      "required": true,
      "disabled": false,
      "hidden": false,
      "isSensitive": false,
      "rely": {
        "operation": "AND",
        "expression": [
          {
            "key": "isSendEmail",
            "value": true
          }
        ]
      }
    }
  },
  "releaseInfo": {
    "projectId": "Demo",
    "name": "uploadReport",
    "language": "java",
    "logoUrl": "${{indexFile(\"uploadReport.png\")}}",
    "category": "TASK",
    "classifyCode": "deploy",
    "jobType": "AGENT",
    "os": [
      "LINUX",
      "MACOS",
      "WINDOWS"
    ],
    "labelCodes": [
    ],
    "summary": "Archive the report files in the specified directory of the builder, and send email notifications at the same time.",
    "description": "${{indexFile(\"README.md\")}}",
    "configInfo": {
      "frontendType": "NORMAL"
    },
    "versionInfo": {
      "publisher": "admin",
      "releaseType": "NEW",
      "version": "1.0.0",
      "versionContent": "init"
    }
  }
}

